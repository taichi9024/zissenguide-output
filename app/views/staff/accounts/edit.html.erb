<%= form_with model: @staffmember, url: :staff_account, method: "patch" do |f| %>

    <div class="form-group">
    <%= f.label :email, "メール"%>
    <%= f.text_field :email,value:"#{@staff_member.email}", class: "form-control" %>
    
    <div class="form-group">
    <%= f.label :family_name, "苗字"%>
    <%= f.text_field :family_name, value:"#{@staff_member.family_name}" , class: "form-control" %>
    
    <div class="form-group">
    <%= f.label :given_name, "氏名"%>
    <%= f.text_field :given_name, value:"#{@staff_member.given_name}", class: "form-control" %>
    
    <%= f.submit "変更する", class:"btn btn-primary" %>
<% end %>