<%= form_with model:@staffmember, url: :admin_staff_member  do |f| %>

<div class="form-group">
<%= f.label :email, "メール"%>
<%= f.text_field :email, class: "form-control" %>

<div class="form-group">
<%= f.label :family_name, "苗字"%>
<%= f.text_field :family_name, class: "form-control" %>

<div class="form-group">
<%= f.label :given_name, "氏名"%>
<%= f.text_field :given_name, class: "form-control" %>

<div class="form-group">
<%= f.label :family_name_kane, "苗字"%>
<%= f.text_field :family_name_kana, class: "form-control" %>

<div class="form-group">
<%= f.label :given_name_kana, "氏名"%>
<%= f.text_field :given_name_kana, class: "form-control" %>

<div class="form-group">
<%= f.label :password, "パスワード"%>
<%= f.password_field :password, class: "form-control" %>

<div class="form-group">
<%= f.label :start_date, "開始日時"%>
<%= f.text_field :start_date, class: "form-control" %>

<div class="form-group">
<%= f.label :start_date, "終了日時"%>
<%= f.text_field :end_date, class: "form-control" %>

<div class="form-group">
<%= f.label :suspend, "資格"%>
<%= f.text_field :suspend, class: "form-control" %>

<%= f.submit "登録する", class:"btn btn-primary" %>
<% end %>